    "moment": "^2.24.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/parser": "^7.14.4",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.15.0",
    "@iceworks/spec": "^1.0.0",
    "babel-plugin-import": "^1.13.3",
    "build-plugin-fusion": "^0.1.0",
